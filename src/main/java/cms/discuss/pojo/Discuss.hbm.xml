<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cms.discuss.pojo.Discuss" table="cms_discuss">
        <id name="discussId" type="java.lang.Integer">
            <column name="discussId" />
            <generator class="native" />
        </id>
         <property name="articleId" type="java.lang.Integer">
            <column name="articleId" />
        </property>
        <property name="usersId" type="java.lang.Integer">
            <column name="usersId" />
        </property>
        <property name="userName" type="java.lang.String">
            <column name="userName" length="20" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" length="65535" />
        </property>
        <property name="isShow" type="java.lang.Short">
            <column name="isShow" />
        </property>
    </class>
</hibernate-mapping>
