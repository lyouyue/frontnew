<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="wxmg.material.pojo.MaterialImageTxtInfo" table="wx_material_image_txt_info">
<!-- 素材图文ID,增长策略自增 -->
<id name="materialImageTxtInfoId" type="java.lang.Integer" >
<column name="materialImageTxtInfoId"/>
<generator class="increment"/>
</id>
<!-- 素材图片信息ID -->
<property name="materialImageInfoId" type="java.lang.Integer">
<column name="materialImageInfoId" />
</property>
<!-- 公众号 -->
<property name="publicNumberId" type="java.lang.Integer">
<column name="publicNumberId" />
</property>
<!-- 父图文信息ID -->
<!-- <property name="parentImtxtInfoId" type="java.lang.Integer"> -->
<!-- <column name="parentImtxtInfoId"/> -->
<!-- </property> -->
<!-- 图文信息排序号 -->
<!-- <property name="imtxtInfoSortNumber" type="java.lang.Integer"> -->
<!-- <column name="imtxtInfoSortNumber" /> -->
<!-- </property> -->
<!-- 素材图文信息ID -->
<property name="imageInfoTypeEnumId" type="java.lang.Integer">
<column name="imageInfoTypeEnumId" />
</property>
<!-- 图文信息类型 -->
<property name="imageTxtMediaId" type="java.lang.String">
<column name="imageTxtMediaId" />
</property>
<!-- 素材图文标题 -->
<property name="materialTitle" type="java.lang.String">
<column name="materialTitle"/>
</property>
<!--  图片摘要描述-->
<property name="picDigestDescription" type="java.lang.String">
<column name="picDigestDescription"/>
</property>
<!--  图片正文描述 -->
<property name="picDescription" type="java.lang.String">
 <column name="picDescription" />
 </property>
<!--  点击链接跳转Url-->
<property name="clickSkipUrl" type="java.lang.String">
<column name="clickSkipUrl"/>
</property>
<!--  作者-->
<property name="author" type="java.lang.String">
<column name="author"/>
</property>
<!--  原文链接-->
<property name="originalLink" type="java.lang.String">
<column name="originalLink"/>
</property>
<!-- 信息是否发送 -->
<property name="infoSendFlag" type="java.lang.Integer">
<column name="infoSendFlag"/>
</property>
<!-- 创建人 -->
<property name="createdByBigint" type="java.lang.Integer">
<column name="createdByBigint" />
</property>
<!-- 创建时间 -->
<property name="createTimeDatetime" type="java.util.Date">
<column name="createTimeDatetime"/>
</property>
<!-- 最后修改人 -->
<property name="lastUpdatedByBigint" type="java.lang.Integer">
<column name="lastUpdatedByBigint" />
</property>
<!-- 最后修改日期 -->
<property name="lastUpdateDateDatetime" type="java.util.Date">
<column name="lastUpdateDateDatetime" />
</property>
<!--删除标记  -->
<property name="deletedFlag" type="java.lang.Integer">
<column name="deletedFlag" />
</property>
<!-- 删除人 -->
<property name="deletedBy" type="java.lang.Integer">
<column name="deletedBy"/>
</property>
<!-- 删除时间 -->
<property name="deletionDate" type="java.util.Date">
<column name="deletionDate"/>
</property>
<!-- 备注 -->
<property name="remark" type="java.lang.String">
<column name="remark"/>
</property>
</class>
</hibernate-mapping>