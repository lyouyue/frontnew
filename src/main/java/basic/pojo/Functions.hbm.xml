<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="basic.pojo.Functions" table="basic_functions">
        <id name="fid" type="java.lang.Integer">
            <column name="fid"/>
            <generator class="native" />
        </id>
        <property name="purviewId" type="java.lang.Integer">
            <column name="purviewId"/>
        </property>
        <property name="funName" type="java.lang.String">
            <column name="funName"/>
        </property>
        <property name="funValue" type="java.lang.String">
            <column name="funValue"/>
        </property>
    </class>
</hibernate-mapping>
