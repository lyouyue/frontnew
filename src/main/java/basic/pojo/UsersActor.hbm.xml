<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="basic.pojo.UsersActor" table="basic_useractor" >
        <id name="userActorId" type="java.lang.Integer">
            <column name="userActorId"/>
            <generator class="native"/>
        </id>
        <property name="usersId" type="java.lang.Integer">
            <column name="usersId"/>
        </property>
        <property name="actorId" type="java.lang.Integer">
            <column name="actorId"/>
        </property>
        <property name="actorName" type="java.lang.String">
            <column name="actorName"/>
        </property>
    </class>
</hibernate-mapping>
