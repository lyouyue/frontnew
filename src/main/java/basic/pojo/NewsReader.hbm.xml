<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="basic.pojo.NewsReader" table="basic_reader_news" >
        <id name="readId" type="java.lang.Integer">
            <column name="readId"/>
            <generator class="native"/>
        </id>
        <property name="newsId" type="java.lang.Integer">
            <column name="newsId"/>
        </property>
         <property name="customerId" type="java.lang.Integer">
            <column name="customerId"/>
        </property>
         <property name="readtime" type="java.util.Date">
            <column name="readtime"/>
        </property>
    </class>
</hibernate-mapping>
