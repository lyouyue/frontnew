<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="shop.common.pojo.ShopSeo" table="shop_seo">
        <id name="shopSeoId" type="java.lang.Integer">
            <column name="shopSeoId"/>
            <generator class="native" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title"/>
        </property>
        <property name="keywords" type="java.lang.String">
            <column name="keywords"/>
        </property>
        <property name="description" type="java.lang.String">
            <column name="description"/>
        </property>
        <property name="categorySource" type="java.lang.String">
            <column name="categorySource"/>
        </property>
        <property name="sourceId" type="java.lang.Integer">
            <column name="sourceId"/>
        </property>
    </class>
</hibernate-mapping>
