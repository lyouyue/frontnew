<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="shop.customer.pojo.Sonaccount" table="shop_sonaccount" >
		<id name="sonAccountId" type="java.lang.Integer">
			<column name="sonAccountId"/>
			<generator class="native"/>
  		</id>
		<property name="customerId" type="java.lang.Integer">
			<column name="customerId"/>
		</property>
		<property name="loginName" type="java.lang.String">
            <column name="loginName"/>
        </property>
        <property name="password" type="java.lang.String">
            <column name="password"/>
        </property>
        <property name="nickname" type="java.lang.String">
            <column name="nickname"/>
        </property>
        <property name="phone" type="java.lang.String">
            <column name="phone"/>
        </property>
        <property name="email" type="java.lang.String">
            <column name="email"/>
        </property>
        <property name="photoUrl" type="java.lang.String">
            <column name="photoUrl"/>
        </property>
        <property name="registerIp" type="java.lang.String">
            <column name="registerIp"/>
        </property>
        <property name="registerDate" type="java.util.Date">
            <column name="registerDate"/>
        </property>
        <property name="lockedState" type="java.lang.Integer">
            <column name="lockedState"/>
        </property>
        <property name="lockedDate" type="java.util.Date">
            <column name="lockedDate"/>
        </property>
        <property name="type" type="java.lang.Integer">
            <column name="type"/>
        </property>
    </class>
</hibernate-mapping>
