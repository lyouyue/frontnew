<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="shop.store.pojo.SupplierMallCoinDetail" table="shop_suppliermallcoindetail" >
		<id name="supplierMallCoinDetailId" type="java.lang.Integer">
			<column name="supplierMallCoinDetailId"/>
			<generator class="native"/>
  		</id>
		<property name="supplierId" type="java.lang.Integer">
            <column name="supplierId"/>
        </property>
        <property name="supplierLoginName" type="java.lang.String">
            <column name="supplierLoginName"/>
        </property>
        <property name="ordersId" type="java.lang.Integer">
            <column name="ordersId"/>
        </property>
        <property name="ordersNo" type="java.lang.String">
            <column name="ordersNo"/>
        </property>
        <property name="tradingTime" type="java.util.Date">
            <column name="tradingTime"/>
        </property>
        <property name="type" type="java.lang.Integer">
            <column name="type"/>
        </property>
        <property name="tradMallCoin" type="java.math.BigDecimal">
            <column name="tradMallCoin"/>
        </property>
        <property name="totalInPut" type="java.math.BigDecimal">
            <column name="totalInPut"/>
        </property>
        <property name="totalOutPut" type="java.math.BigDecimal">
            <column name="totalOutPut"/>
        </property>
    </class>
</hibernate-mapping>
