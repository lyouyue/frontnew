<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="shop.common.pojo.DistrictInfo" table="basic_districtinfo">
        <id name="districtInfoId" type="java.lang.Integer">
            <column name="districtInfoId"/>
            <generator class="native" />
        </id>
        <property name="districtName" type="java.lang.String">
            <column name="districtName"/>
        </property>
        <property name="sortCode" type="java.lang.Integer">
            <column name="sortCode"/>
        </property>
        <property name="parentId" type="java.lang.Integer">
            <column name="parentId"/>
        </property>
        <property name="loadType" type="java.lang.String">
            <column name="loadType"/>
        </property>
    </class>
</hibernate-mapping>
