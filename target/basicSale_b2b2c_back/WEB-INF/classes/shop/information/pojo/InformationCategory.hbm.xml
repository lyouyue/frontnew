<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="shop.information.pojo.InformationCategory" table="shop_information_category" dynamic-insert="true" dynamic-update="true">
        <id name="categoryId" type="java.lang.Integer">
            <column name="categoryId"/>
            <generator class="native" />
        </id>
        <property name="customerId" type="java.lang.Integer">
            <column name="customerId"/>
        </property>
        <property name="shopInfoId" type="java.lang.Integer">
            <column name="shopInfoId"/>
        </property>
        <property name="shopProCategoryName" type="java.lang.String">
            <column name="shopProCategoryName"/>
        </property>
        <property name="sortCode" type="java.lang.Integer">
            <column name="sortCode"/>
        </property>
        <property name="parentId" type="java.lang.Integer">
            <column name="parentId"/>
        </property>
        <property name="isShow" type="java.lang.Integer">
            <column name="isShow"/>
        </property>
        <property name="categoryImage" type="java.lang.String">
            <column name="categoryImage"/>
        </property>
        <property name="categoryDescription" type="java.lang.String">
            <column name="categoryDescription"/>
        </property>
        <property name="loadType" type="java.lang.String">
            <column name="loadType"/>
        </property>
        <property name="level" type="java.lang.Integer">
            <column name="level"/>
        </property>
    </class>
</hibernate-mapping>
