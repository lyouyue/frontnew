<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 使用场合标签 -->
<!-- mqr -->
<hibernate-mapping>
    <class name="shop.tags.pojo.ShopProductTradeSituationTag" table="shop_product_trade_situation_tag" >
    	<!-- 商品适合行业使用场合标签ID -->
        <id name="ptstId" type="java.lang.Integer">
            <column name="ptstId"/>
            <generator class="native"/>
        </id>
        <!-- 商品实体ID -->
        <property name="productId" type="java.lang.Integer">
            <column name="productId"/>
        </property>
     	 <!-- 适合行业标签ID -->
        <property name="ttId" type="java.lang.Integer">
            <column name="ttId"/>
        </property>
     	 <!-- 使用场合标签ID -->
        <property name="stId" type="java.lang.Integer">
            <column name="stId"/>
        </property>
    </class>
</hibernate-mapping>
